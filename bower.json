{
  "name": "ember-localforage-adapter",
  "version": "2.1.0",
  "homepage": "https://github.com/genkgo/ember-localforage-adapter",
  "authors": [
    "Ryan Florence <rpflorence@gmail.com>",
    "Frederik Bosch <f.bosch@genkgo.nl>",
    "Jildert Miedema <jildertmiedema@gmail.com>"
