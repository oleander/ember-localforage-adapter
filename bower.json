    "ember": "2.1.x",
    "ember-data": "2.1.x",
    "ember-resolver": "~0.1.14",
    "loader.js": "ember-cli/loader.js#3.3.x",
    "ember-cli-shims": "ember-cli/ember-cli-shims#~0.0.5",
    "ember-cli-test-loader": "ember-cli/ember-cli-test-loader#0.2.x",
    "ember-load-initializers": "ember-cli/ember-load-initializers#~0.1.6",
    "ember-qunit": "~0.4.10",
    "ember-qunit-notifications": "0.1.x",
    "qunit": "1.19.x"
  }
